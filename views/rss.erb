<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0">

  <channel>

    <title>SEAOO</title>
    <link>http://www.seaoo.ca</link>
    <language>fr-CA</language>
    <description>Soyez automatiquement informé lorsque de nouveaux appels d'offres sont publiés sur seao.ca</description>

    <% @notices.each do |notice| %>
    <item>
      <pubDate><%= notice.date %></pubDate>
      <title><%= notice.title %></title>
      <link><%= notice.url %></link>
      <category><%= notice.category %></category>
      <description><%= notice.description %></description>
    </item>
    <% end %>

  </channel>

</rss>